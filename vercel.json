{
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "x-routes-probe",
          "value": "1"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/transactions",
      "destination": "/?network=mainnet"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}